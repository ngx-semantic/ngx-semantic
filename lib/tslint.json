{
  "extends": "../tslint.json",
  "rules": {
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      "sui",
      "kebab-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "object-literal-sort-keys": [
      false
    ]
  }
}
