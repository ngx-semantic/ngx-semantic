{
  "extends": "../tslint.json",
  "rules": {
    "component-selector": [
      true,
      [
        "element",
        "attribute"
      ],
      "sui",
      "kebab-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false
  }
}
